<!DOCTYPE html>
<html lang="en">
<head>
   <meta charset="UTF-8">
   <meta http-equiv="X-UA-Compatible" content="IE=edge">
   <meta name="viewport" content="width=device-width, initial-scale=1.0">
   <title>dashboard</title>

   <!-- font awesome cdn link  -->
   <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css">
      <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" >

   <!-- custom css file link  -->
   <link rel="stylesheet" href="~/css/edit-style.css">
   <style>
       body{
           background-color:orange;
       }
        #content
        {
           flex-grow: 1;
           height:100%;
           width:100%;
           margin:0 auto;
           overflow: auto; 
           font-size:16px;
        }
       .logo a{
      font-size: 25px;
      color:black;
      margin-left:100px;
      margin-top:10px;
      text-decoration:underline;
   }
.logo a:hover{
    color:orangered;
       }
   </style>
    @await RenderSectionAsync("stylecss", required: false)
</head>
<body>
<div class="logo">
    <a asp-action="Index" asp-controller="Home" >yum-yum 😋</a>
</div>
<div>
    <a asp-action="Index" asp-controller="Admin" class="btn btn-secondary" style="margin:25px 0 0 100px">Back to Dashboard</a>
</div>


@RenderBody();



<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/jquery-easing/jquery.easing.min.js"></script>
<script src="https://unpkg.com/swiper@8/swiper-bundle.min.js"></script>
<script src="~/js/admin_script.js"></script>
 @await RenderSectionAsync("Scripts", required: false)
 </body>
</html>